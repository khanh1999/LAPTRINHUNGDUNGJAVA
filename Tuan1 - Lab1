public class BTlap11 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Ho va ten: ");
        String hoTen = scanner.nextLine();
        System.out.print("Diem TB: ");
        double diemTB = scanner.nextDouble();
        System.out.printf("%s: %f diem ",hoTen,diemTB);
        System.out.println();
        System.out.print("Chieu dai: ");
        int dai = scanner.nextInt();
        System.out.print("Chieu rong: ");
        int rong = scanner.nextInt();
        int DT = dai*rong;
        int min = Math.min(dai, rong);
        System.out.printf("Dien tich: %d",DT);
        System.out.println();
        System.out.printf("Canh nho nhat: %d",min);
        System.out.println();
        double TT = Math.pow(dai,3);
        System.out.printf("The tich khoi chu nhat: %f ",TT);
        System.out.println();
        System.out.print("He so a,b,c: ");
        int a = scanner.nextInt();
        int b = scanner.nextInt();
        int c = scanner.nextInt();
        int delta = b*b-4*a*c;
        double can = Math.sqrt(delta);
        System.out.printf("Delta = :%d\nCan Delta = %f", delta,can);
        
        
    }
    
}
